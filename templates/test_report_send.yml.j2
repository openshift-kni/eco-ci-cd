---
# This is variables file for test_report_send.yml playbook.
# It was generated on {{ current_date }} at {{ current_time }} by user '{{ username }}'
splunk_url: "{{ splunk_url }}"
splunk_token: "{{ splunk_token }}"
{% if splunk_channel | length > 0 -%}
splunk_channel: "{{ splunk_channel }}"
{% endif -%}
{% if splunk_index | length > 0 %}
splunk_index: "{{ splunk_index }}"
{% endif %}

trs_ci_system: "{{ trs_ci_system }}"
{% if reports_list | length > 0 %}
junit2_input_reports_list:
{%- for fname in reports_list %}
  - "{{ fname }}"
{%- endfor %}
{% endif %}
{% if trs_metadata_path | length > 0 %}
trs_metadata_path: "{{ trs_metadata_path }}"
{% endif %}
 
# Directory for any output generated by the junit2json processing
junit2_output_dir: "{{ output_dir | default('{{ playbook_dir }}/output_data') }}"

# --- End of Example Variables ---
